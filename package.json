{
  "name": "shopping-cidadao",
  "version": "1.0.4",
  "description": "Challenge Shopping Cidadao",
  "keywords": [
    "API",
    "Back-end",
    "Express",
    "Front-end",
    "Mysql",
    "NodeJS",
    "ORM",
    "ReactJS",
    "REST",
    "SCSS",
    "Sequelize",
    "Sticker App",
    "Styled Components"
  ],
  "repository": "https://github.com/vitorblsantos/shopping-cidadao",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "12.x",
    "yarn": "1.22.x"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production concurrently --kill-others-on-fail \"rm -rf ./build && webpack --mode=production && sucrase ./server -d ./build/server/ --transforms imports\"",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others-on-fail \"webpack-dev-server --open --mode development -d\" \"nodemon --exec sucrase-node ./server \"",
    "lint": "concurrently --kill-others-on-fail \"eslint ./client/**/*.{js,jsx}\" \"eslint ./server/**/*.js\"",
    "sequelize:migrate": "sequelize db:migrate",
    "sequelize:migrate:undo": "sequelize db:migrate:undo:all",
    "sequelize:seed": "sequelize db:seed:all --debug",
    "sequelize:seed:undo": "sequelize db:seed:undo:all",
    "start": "node ./build/server",
    "server": "cross-env NODE_ENV=development sucrase-node ./server/",
    "server:debug": "cross-env NODE_ENV=development nodemon --exec sucrase-node ./server/"
  },
  "dependencies": {
    "@babel/core": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.13.10",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "core-js": "3",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ibm-watson": "^6.1.1",
    "mysql2": "^2.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sequelize": "^6.6.5",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webpack": "^4.41.21",
    "chokidar": "^3.4.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.1",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.20.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "sass": "^1.35.1",
    "sass-loader": "^8.0.2",
    "sequelize-cli": "^6.2.0",
    "source-map-resolve": "^0.6.0",
    "style-loader": "^1.2.1",
    "styled-components": "^5.2.1",
    "sucrase": "^3.15.0",
    "webpack-manifest-plugin": "^3.1.0"
  },
  "resolutions": {
    "chokidar": "^3.4.0"
  }
}
